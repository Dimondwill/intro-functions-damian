#factor calc (short for calculator)
'''num = input("Enter a number and I shall tell you every one of its factors: ")
num = int(num)

def factors(number):
    result = []
    for i in range(1, number + 1):
        if number % i == 0:
            result.append(i)
    return result
print(factors(num))'''
#gcf calc (short for calculator) no idea how this one works
'''print("enter 2 numbers in and i will tell you the greatest common factor")
num1 = int(input("Enter first number: "))
num2 = int(input("Enter second number: "))

def gcd(a, b):
    while b:
        a, b = b, a % b
    return a

print(f"The gcf of {num1} and {num2} is {gcd(num1, num2)}")'''
